package com.zhirong.ncdata.config;

import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.context.annotation.Configuration;
import org.springframework.context.annotation.PropertySource;
import org.springframework.stereotype.Component;

@Configuration
@ConfigurationProperties(prefix = "custom")
@PropertySource("classpath:config.properties")
@Component
public class ConfigProperties {
    //法院裁决文书
    private String GETENTJUDGMENTLIST;
    //法院公告信息
    private String GETANNOUNCEMENTLIST;
    //失信被执行人
    private String GETSHIXINLIST;
    //失信信息
    private String GETZHIXINGLIST;
    //企业工商信息
    private String GETENTESTINFO;
    //对外投资信息
    private String GETINVESTMENTS;
    //新闻舆情
    private String GETNEWS;
    //裁判详细信息
    private String GETJUDGMENTDETAILBYID;
    //招聘信息(经营信息)
    private String GETENTJOBSLIST;
    //商标
    private String GETENTTRADEMARKLIST;
    // 专利
    private String GETENTPATENTLIST;
    // 软著
    private String GETENTCOPYRIGHTLIST;
    // 证书
    private String GETENTCERTIFICATIONLIST;

    public String getGETENTJUDGMENTLIST() {
        return GETENTJUDGMENTLIST;
    }

    public void setGETENTJUDGMENTLIST(String GETENTJUDGMENTLIST) {
        this.GETENTJUDGMENTLIST = GETENTJUDGMENTLIST;
    }

    public String getGETANNOUNCEMENTLIST() {
        return GETANNOUNCEMENTLIST;
    }

    public void setGETANNOUNCEMENTLIST(String GETANNOUNCEMENTLIST) {
        this.GETANNOUNCEMENTLIST = GETANNOUNCEMENTLIST;
    }

    public String getGETSHIXINLIST() {
        return GETSHIXINLIST;
    }

    public void setGETSHIXINLIST(String GETSHIXINLIST) {
        this.GETSHIXINLIST = GETSHIXINLIST;
    }

    public String getGETZHIXINGLIST() {
        return GETZHIXINGLIST;
    }

    public void setGETZHIXINGLIST(String GETZHIXINGLIST) {
        this.GETZHIXINGLIST = GETZHIXINGLIST;
    }

    public String getGETENTESTINFO() {
        return GETENTESTINFO;
    }

    public void setGETENTESTINFO(String GETENTESTINFO) {
        this.GETENTESTINFO = GETENTESTINFO;
    }

    public String getGETINVESTMENTS() {
        return GETINVESTMENTS;
    }

    public void setGETINVESTMENTS(String GETINVESTMENTS) {
        this.GETINVESTMENTS = GETINVESTMENTS;
    }

    public String getGETNEWS() {
        return GETNEWS;
    }

    public void setGETNEWS(String GETNEWS) {
        this.GETNEWS = GETNEWS;
    }

    public String getGETJUDGMENTDETAILBYID() {
        return GETJUDGMENTDETAILBYID;
    }

    public void setGETJUDGMENTDETAILBYID(String GETJUDGMENTDETAILBYID) {
        this.GETJUDGMENTDETAILBYID = GETJUDGMENTDETAILBYID;
    }

    public String getGETENTJOBSLIST() {
        return GETENTJOBSLIST;
    }

    public void setGETENTJOBSLIST(String GETENTJOBSLIST) {
        this.GETENTJOBSLIST = GETENTJOBSLIST;
    }

    public String getGETENTTRADEMARKLIST() {
        return GETENTTRADEMARKLIST;
    }

    public void setGETENTTRADEMARKLIST(String GETENTTRADEMARKLIST) {
        this.GETENTTRADEMARKLIST = GETENTTRADEMARKLIST;
    }

    public String getGETENTPATENTLIST() {
        return GETENTPATENTLIST;
    }

    public void setGETENTPATENTLIST(String GETENTPATENTLIST) {
        this.GETENTPATENTLIST = GETENTPATENTLIST;
    }

    public String getGETENTCOPYRIGHTLIST() {
        return GETENTCOPYRIGHTLIST;
    }

    public void setGETENTCOPYRIGHTLIST(String GETENTCOPYRIGHTLIST) {
        this.GETENTCOPYRIGHTLIST = GETENTCOPYRIGHTLIST;
    }

    public String getGETENTCERTIFICATIONLIST() {
        return GETENTCERTIFICATIONLIST;
    }

    public void setGETENTCERTIFICATIONLIST(String GETENTCERTIFICATIONLIST) {
        this.GETENTCERTIFICATIONLIST = GETENTCERTIFICATIONLIST;
    }
}
